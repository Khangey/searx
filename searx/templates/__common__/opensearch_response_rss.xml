<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0"
     xmlns:opensearch="http://a9.com/-/spec/opensearch/1.1/"
     xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ instance_name }}: {{ q|e }}</title>
    <link>{{ base_url }}?q={{ q|e }}</link>
    <description>Search results for "{{ q|e }}" - {{ instance_name }}</description>
    <item>
      <title>Error</title>
      <description>OpenSearch was closed.</description>
    </item>
  </channel>
</rss>
